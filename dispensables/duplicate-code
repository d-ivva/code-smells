//com code smell 
public class Main {
    public static void main(String[] args) {
        ProfessorTemporario temp = new ProfessorTemporario("Fernando Rodrigues Gabriel", "12345678900", 6000.0, 0, 150.0);
        temp.imprimirSalario();
    }
}
 
class Professor {
    private String nome, cpf;
    private double salarioBase;

    public Professor(String nome, String cpf, double salarioBase) {
        this.nome = nome;
        this.cpf = cpf;
        this.salarioBase = salarioBase;
    }

    public double calcularSalario() {
        return salarioBase;
    }

    public void imprimirSalario() {
        System.out.println("Salário do professor " + nome + ": R$ " + calcularSalario());
    }
}

class ProfessorTemporario {
    private String nome, cpf;
    private double salarioBase, valorHora;
    private int horasExtrasTrabalhadas;

    public ProfessorTemporario(String nome, String cpf, double salarioBase, int horasTrabalhadas, double valorHora) {
        this.nome = nome;
        this.cpf = cpf;
        this.salarioBase = salarioBase;
        this.horasExtrasTrabalhadas = horasExtrasTrabalhadas;
        this.valorHora = valorHora;
    }

    public double calcularSalario() {
        return salarioBase + (horasExtrasTrabalhadas * valorHora);
    }

    public void imprimirSalario() {
        System.out.println("Salário do professor temporário " + nome + ": R$ " + calcularSalario());
    }
}
